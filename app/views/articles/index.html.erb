<div id="banner"><%= link_to 'Wikinominous', articles_path, class: 'homepage' %></div>
<div class="container">
  <h1>Wikinimous</h1>

  <article>
    <% @articles.each do |article| %>
    <h2><%= link_to article.title, article_path(article) %></h2>
    <p><%= article.content %></p>
    <% end %>
  </article>

  <p><%= link_to 'Add a new article', new_article_path %></p>
</div>
